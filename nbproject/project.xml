<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>SAAJ Reference Implementation</name>
            <properties/>
            <folders>
                <source-folder>
                    <label>src/java</label>
                    <type>java</type>
                    <location>src/java</location>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>wspack</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="javadoc">
                    <target>wspackdocs</target>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>wspack</target>
                </action>
                <action name="test">
                    <target>run-tests</target>
                </action>
            </ide-actions>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>src/java</label>
                        <location>src/java</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="clean"/>
                    <ide-action name="javadoc"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="test"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/2">
            <compilation-unit>
                <package-root>src/java</package-root>
                <classpath mode="compile">lib/activation.jar:lib/ant.jar:lib/FastInfoset.jar:lib/jax-qname.jar:lib/junit.jar:lib/maven-repository-importer.jar:lib/optional.jar:lib/saaj-api.jar:lib/servlet.jar</classpath>
                <source-level>1.4</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
